!!!             X F T
Xft.antialias:      true

!!!             X T E R M
xterm*termName:     xterm-256color
XTerm.font:         xft:xos4 Terminus:size=12:style=regular
XTerm.boldFont:     xft:xos4 Terminus:size=12:style=regular

!!!             U R X V T
!
URxvt*termName:     rxvt-unicode-256color
!
! Fonts
URxvt*imLocale:     en_US.UTF-8
Urxvt*buffered:     false
URxvt.intensityStyles: false
URxvt.letterSpace:  0
URxvt.*font:        xft:xos4 Terminus:size=12:style=regular, \
                    xft:Monospace:style=Medium:size=12, \
                    xft:UbuntuMono Nerd Font:size=12, \
                    xft:Inconsolata Nerd Font Mono:size=12, \
                    xft:DejaVuSansMono Nerd Font:size=12, \
                    xft:DroidSansMono Nerd Font:size=12, \
                    xft:FuraMono Nerd Font:size=12, \
                    xft:RobotoMono Nerd Font:size=12, \
                    xft:Noto Sans Mono:size=12
URxvt.*boldFont:    xft:xos4 Terminus:size=12:style=bold, \
                    xft:Monospace:style=Medium:size=12:style=bold, \
                    xft:UbuntuMono Nerd Font:size=12:style=bold, \
                    xft:Inconsolata Nerd Font Mono:size=12:style=bold, \
                    xft:DejaVuSansMono Nerd Font:size=12:style=bold, \
                    xft:DroidSansMono Nerd Font:size=12:style=bold, \
                    xft:FuraMono Nerd Font:size=12:style=bold, \
                    xft:RobotoMono Nerd Font:size=12:style=bold, \
                    xft:Noto Sans Mono:size=12:style=bold
!
! Scrolling
URxvt.saveLines:    1000
URxvt.scrollBar:    false
!
! Transparency
URxvt.transparent:  true
URxvt.shading:      30
!
! Shell
URxvt*loginShell:   true
!
! resize-font
URxvt.resize-font.smaller: C-j
URxvt.resize-font.bigger: C-k

!!!             P A L E T T E
!
!!! Special
!
! URxvt
URxvt.cursorColor:  white
URxvt*foreground:   #999C96
URxvt*background:   #000000
URxvt*cursorColor:  #999C96
!
! XTerm
XTerm*foreground:   #999C96
XTerm*background:   #000000
XTerm*cursorColor:  #999C96
!
!!! Colors
!
! black
*.color0:           #073642
*.color8:           #002B36
!
! red
*.color1:           #DC322F
*.color9:           #CB4B16
!
! green
*.color2:           #859900
*.color10:          #859900
!
! yellow
