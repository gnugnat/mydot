;; Quicklisp
;; WARNING! I set it up in ~/.local/share/quicklisp/setup.lisp

#-quicklisp

(let
    ((quicklisp-init
       (merge-pathnames
        ".local/share/quicklisp/setup.lisp" (user-homedir-pathname))
       ))
  (when (probe-file quicklisp-init)
    (load quicklisp-init)
    )
  )


